    "tokenColors": [
        {
            "scope": ["meta.embedded", "source.groovy.embedded"],
            "settings": {
                "foreground": "#e9e9e9"
            }
        },
        {
            "scope": ["punctuation.definition.template-expression"],
            "settings": {
                "foreground": "#ac5685"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#666",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#97bbd1"
            }
        },
        {
            "name": "String Template",
            "scope": "string.template",
            "settings": {
                "foreground": "#97bbd1"
            }
        },
        {
            "name": "String Template",
            "scope": [
                "punctuation.definition.string.template.end",
                "punctuation.definition.string.template.begin"
            ],
            "settings": {
                "foreground": "#dd6236"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#9b3867"
            }
        },
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#666eb6"
            }
        },
        {
            "name": "User-defined constant",
            "scope": "constant.character, constant.other",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#e2e2e2"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": "",
                "foreground": "#67a3c8"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
