        {
            "name": "Library variable",
            "scope": "support.other.variable",
            "settings": {
                "foreground": "#f8eb82"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "fontStyle": "",
                "foreground": "#b84b44"
            }
        },
        {
            "name": "Invalid deprecated",
