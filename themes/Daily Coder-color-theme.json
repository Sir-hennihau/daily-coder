                "foreground": "#4c87c1"
            }
        },
        {
            "name": "Storage type",
            "scope": "storage.type",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#e2e2e2"
            }
        },
        {
            "name": "Class name",
            "scope": "entity.name.type, entity.name.class",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#e2e2e2"
